[Desktop Entry]
Type=Application                          # Indicates the type as listed above
Version=0.1.0                               # The version of the desktop entry specification to which this file complies
Name=Wonder Calendar                            # The name of the application
Comment=Just another calendar    # A comment which can/will be used as a tooltip
Exec=wondercal                            # The executable of the application.
Icon=/opt/wondercal/icon.png                            # The name of the icon that will be used to display this entry
Terminal=false                            # Describes whether this application needs to be run in a terminal or not
Name[en_US]=Wonder Calendar.desktop
